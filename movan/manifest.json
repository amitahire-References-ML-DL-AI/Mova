{
"manifest_version": 2,
"name": "Mova",
"description": "Mova: Movement Analytics Platform",
"version": "0.7",
"content_security_policy": "script-src 'self' unsafe-inline",
"app": {
 "background": {
      "scripts": ["chromeapp/background.js"]
    }
},
"permissions": [
"unlimitedStorage",
"notifications",
"fileBrowserHandler",
"fileSystem",
"fileSystemProvider"
],
"icons": {
    "128": "img/mova-logo.png"
  }
}